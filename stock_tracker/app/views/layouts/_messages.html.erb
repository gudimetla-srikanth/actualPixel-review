<div class="container">
  <div class="container">
    <%if alert %>
    <div class="alert alert-danger alert-dismissible fade show own-pos-m" role="alert">
      <strong><%=alert%></strong>
      <button type="button" class=" bg-transparent own-pos-mb " data-bs-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
  <%end%>
  <%if notice %>
    <div class="alert alert-success alert-dismissible fade show own-pos-m" role="alert">
      <strong><%=notice%></strong>
      <button type="button" class=" bg-transparent own-pos-mb" data-bs-dismiss="alert" aria-label="Close">
        <span aria-hidden="true" class="own-mark">&times;</span>
      </button>
    </div>
  <%end%>
  </div>
</div>
