<table class="table">
  <thead>
    <tr>
      <th scope="col">Ticker</th>
      <th scope="col">Name</th>
      <th scope="col">Last Price</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @tracked_stocks.each do |stock| %>
    <tr>
      <th scope="row"><%= stock.ticker %></th>
      <td><%= stock.name %></td>
      <td><%= stock.last_price %></td>
      <td>Action</td>
    </tr>
    <%end%>
  </tbody>
</table>